   "source": [
    "import cv2\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "image_path = \"./img/andeng.bmp\"\n",
    "image = cv2.imread(image_path)\n",
    "\n",
    "plt.imshow(cv2.cvtColor(image, cv2.COLOR_BGR2RGB))\n",
    "plt.axis('off')\n",
    "plt.show()\n",
    "\n",
    "output_path = \"./img/test.bmp\"\n",
    "cv2.imwrite(output_path, image)"
   ]
